---
import Layout from "../layouts/Layout.astro";
import { getCollection } from "astro:content";
const posts = await getCollection("projects");
---

<Layout title="Arden Sinclair">
  {
    posts.map(async (post) => {
      const { Content } = await post.render();
      return (
        <article>
          <Content />
        </article>
      );
    })
  }
</Layout>
